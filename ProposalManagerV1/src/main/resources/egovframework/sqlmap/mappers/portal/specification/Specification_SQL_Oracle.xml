<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="specification" >
<!-- MyBatis 3.2.6 after : Supports returning multiple fields in a select key : ref. http://blog.mybatis.org/2014/03/mybatis-326-released.html -->
<!-- <typeAlias  alias="SurveryENVVO" type="org.cloud.manager.web.impl.SurveryENVVO" /> 
<typeAlias  alias="SurveryItemVO" type="org.cloud.manager.web.impl.SurveryItemVO" />
<typeAlias  alias="SurveryResultVO" type="org.cloud.manager.web.impl.SurveryResultVO" />
 -->
<!-- 1.1.서비스 정보 등록 -->
<insert id="specification.insertServiceInfo" parameterType="String" useGeneratedKeys="true" keyProperty="bNo" >

</insert>
<!-- 1.2.오퍼레이션 정보 등록 -->
<insert id="specification.insertOpInfo" parameterType="String" useGeneratedKeys="true" keyProperty="bNo" >


</insert>
<!-- 1.3.응답 메시지 정보 등록 -->
<insert id="specification.inserMSGRspsInfos" parameterType="String" useGeneratedKeys="true" keyProperty="bNo" >


</insert>
<!-- 1.4.요청 메시지 정보 등록 -->
<insert id="specification.insertMSGReqsInfos" parameterType="String" useGeneratedKeys="true" keyProperty="bNo" >


</insert>
<!-- 메시지명세 등록 : 요청, 응답 구분 -->
<insert id="specification.msgInfoInsert">
    <selectKey keyProperty="userId, userName" resultType="hashmap" order="BEFORE">
        SELECT 'key1' as userId, 'key2' as userName FROM DUAL
    </selectKey>
    INSERT INTO USERS
    (
        USER_ID
      , NAME
      , PASSWORD
      , AGE
    ) VALUES
    (
        #{userId}
      , #{userName}
      , #{password}
      , #{age}
    )
</insert>
</mapper>